{
  "name": "Siclos",
  "version": "0.0.1",
  "description": "Siclos NestJs Mongo Boilerplate",
  "author": "Rcode",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format-check": "prettier --check \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles ",
    "test:cov": "yarn test --coverage ",
    "test:staged": "yarn test  --bail --findRelatedTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand ",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "pre-commit": "lint-staged",
    "migration:test": "seed -u 'mongodb://inspire_mongodb_test:27017/inspire_test' --transpile-only --set-timestamps --drop-collections ./src/migration",
    "migration": "seed -u 'mongodb://inspire_mongodb:27017/inspire' --transpile-only --set-timestamps --drop-collections ./src/migration"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^8.0.8",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/platform-fastify": "^7.6.18",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.8.2",
    "@sendgrid/mail": "^7.4.6",
    "@sentry/node": "^6.14.0",
    "@types/express": "^4.17.13",
    "@types/nodemailer": "^6.4.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "fastify": "^3.18.0",
    "fastify-compress": "^3.6.0",
    "fastify-helmet": "^5.3.1",
    "fastify-multer": "^2.0.2",
    "fastify-multipart": "^5.1.0",
    "firebase-admin": "^9.11.1",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21",
    "mongo-seeding": "^3.7.0",
    "mongo-seeding-cli": "^3.7.0",
    "mongoose": "^5.13.8",
    "mongoose-delete": "^0.5.3",
    "multer": "^1.4.4",
    "nestjs-command": "^2.0.1",
    "nodemailer": "^6.6.3",
    "passport-firebase-jwt": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.10",
    "@types/cron": "^1.7.2",
    "@types/faker": "^5.5.8",
    "@types/jest": "^26.0.23",
    "@types/mongoose-delete": "^0.5.5",
    "@types/node": "^15.12.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^5.5.3",
    "fastify-static": "^4.2.2",
    "fastify-swagger": "^4.8.0",
    "husky": "^5.0.0",
    "jest": "^27.1.0",
    "lint-staged": "^11.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "point-of-view": "^4.15.0",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">= 14.16.1",
    "yarn": ">= 1.22.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "roots": [
      "../test"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
